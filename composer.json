{
    "name": "khill/datatableplus",
    "description": "DataTablePlus is an add-on package for Lavacharts which extends the functionality of DataTables.",
    "keywords": [
        "charts",
        "graphs",
        "google"
    ],
    "homepage": "http://lavacharts.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Hill",
            "email": "kevinkhill@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "league/csv": "~7.1"
    },
    "autoload": {
        "psr-4": {
            "Khill\\Lavacharts\\DataTablePlus\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.3",
        "sami/sami": "~3.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Khill\\Lavacharts\\DataTablePlus\\Tests\\": "tests/"
        }
    },
}
